# tile keybinds for khd

# horizontal tile presets
cmd + lctrl - z : tile 0-1
cmd + lctrl - x : tile 1-2
cmd + lctrl - c : tile 2-3
cmd + lctrl - v : tile 3-4

cmd + lctrl - a : tile 0-2
cmd + lctrl - s : tile 1-3
cmd + lctrl - d : tile 2-4

cmd + lctrl - q : tile 0-3
cmd + lctrl - w : tile 1-4

cmd + lctrl - r : tile -

# vertical tile presets
cmd + lctrl - f : tile f
cmd + lctrl - t : tile t
cmd + lctrl - b : tile b
cmd + lctrl - y : tile y
cmd + lctrl - h : tile h
cmd + lctrl - n : tile n
cmd + lctrl - j : tile j
cmd + lctrl - k : tile k

# snap presets
cmd + lctrl - 0 : tile snap 0
cmd + lctrl - 1 : tile snap 1
cmd + lctrl - 2 : tile snap 2
cmd + lctrl - 3 : tile snap 3
cmd + lctrl - 4 : tile snap 4

# move between multiple displays
cmd + lctrl - 0x21 : tile [
cmd + lctrl - 0x1E : tile ]

# undo
cmd + lctrl - u : tile undo
